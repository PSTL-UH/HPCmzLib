# Generated automatically from Makefile.in by configure.

MZLIB_DIR      = @mzlib_dir@
MZLIB_INC_DIR  = $(MZLIB_DIR)/include 
MZLIB_LIB_DIR  = $(MZLIB_DIR)/lib
MZLIB_LIB      = libmzlib.a
FLASHLFQ_LIB   = libflashlfq.a

XSD_DIR = /usr
XSD_INC_DIR = $(XSD_DIR)/include
XSD_LIB_DIR = $(XSD_DIR)/lib64
XSD_LIB = -lxerces-c

#Set some Flags
AR	    = ar
AR_FLAGS    = clr

CC          = @CC@
CFLAGS      = @CFLAGS@


CXX         = @CXX@
CXXFLAGS    = @CXXFLAGS@ -I$(MZLIB_INC_DIR) -I$(MZLIB_DIR)/MzLibUtil -I$(XSD_INC_DIR)/xsd -I$(XSD_INC_DIR)/xercesc -I/usr/include/libxml2
CXXLOCALFLAGS = -DNDEBUG
LDFLAGS     = -L$(MZLIB_LIB_DIR)  -lflashlfq -lmzlib -lz -lstdc++fs -lxml2 -L$(XSD_LIB_DIR) $(XSD_LIB)

